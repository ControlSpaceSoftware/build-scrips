{
  "name": "build-scripts",
  "version": "1.0.28",
  "description": "build node module scripts",
  "main": "index.js",
  "bin": {
    "build-scripts": "./index.js"
  },
  "scripts": {
	  "commit:all:push": "git add --all . && git commit -a -m 'wip' && git push origin",
    "build:lambda": "./node_modules/gulp/bin/gulp.js",
    "build": "babel src --out-dir lib",
    "test": "mocha --compilers js:babel-core/register test",
    "test:watch": "mocha --compilers js:babel-core/register --watch test",
    "commit": "npm run build && npm run test && git commit",
    "patch": "npm version patch",
    "push": "git push origin --follow-tags",
    "patch:push": "npm run build && npm run test && npm run patch && npm run push"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ControlSpaceSoftware/build-scripts.git"
  },
  "author": "javadoug",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/ControlSpaceSoftware/build-scripts/issues"
  },
  "homepage": "https://github.com/ControlSpaceSoftware/build-scripts#readme",
  "dependencies": {
    "aws-sdk": "^2.96.0",
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-preset-env": "^1.6.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-es2015-rollup": "^3.0.0",
    "chalk": "^2.1.0",
    "commander": "^2.11.0",
    "del": "^3.0.0",
    "gulp": "github:gulpjs/gulp#4.0",
    "gulp-file": "^0.3.0",
    "gulp-install": "^1.1.0",
    "gulp-rename": "^1.2.2",
    "gulp-zip": "^4.0.0",
    "lodash.camelcase": "^4.3.0",
    "lodash.kebabcase": "^4.1.1",
    "rollup": "^0.47.6",
    "rollup-plugin-babel": "^3.0.2",
    "shelljs": "^0.7.8",
    "chai": "^4.1.1",
    "expect": "^1.20.2",
    "mocha": "^3.5.0",
    "sinon": "^3.2.1"
  },
  "devDependencies": {},
  "aws": {
    "region": "us-east-1"
  },
  "globals": {
    "aws-sdk": "AWS",
    "shortid": "shortId"
  },
  "babel": {
    "presets": [
      "env"
    ]
  }
}
